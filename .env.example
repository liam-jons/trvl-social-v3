# Supabase Configuration (Required for Authentication)
VITE_SUPABASE_URL="your_supabase_project_url"         # Format: https://xxxxx.supabase.co
VITE_SUPABASE_PUBLISHABLE_KEY="your_supabase_anon_key" # Format: eyJhbGciOiJ...

# API Keys (Required to enable respective provider)
ANTHROPIC_API_KEY="your_anthropic_api_key_here"       # Required: Format: sk-ant-api03-...
PERPLEXITY_API_KEY="your_perplexity_api_key_here"     # Optional: Format: pplx-...
OPENAI_API_KEY="your_openai_api_key_here"             # Optional, for OpenAI models. Format: sk-proj-...
GOOGLE_API_KEY="your_google_api_key_here"             # Optional, for Google Gemini models.
MISTRAL_API_KEY="your_mistral_key_here"               # Optional, for Mistral AI models.
XAI_API_KEY="YOUR_XAI_KEY_HERE"                       # Optional, for xAI AI models.
GROQ_API_KEY="YOUR_GROQ_KEY_HERE"                     # Optional, for Groq models.
OPENROUTER_API_KEY="YOUR_OPENROUTER_KEY_HERE"         # Optional, for OpenRouter models.
AZURE_OPENAI_API_KEY="your_azure_key_here"            # Optional, for Azure OpenAI models (requires endpoint in .taskmaster/config.json).
OLLAMA_API_KEY="your_ollama_api_key_here"             # Optional: For remote Ollama servers that require authentication.
GITHUB_API_KEY="your_github_api_key_here"             # Optional: For GitHub import/export features. Format: ghp_... or github_pat_...

# Maps and Location Services (Required for Map Features)
VITE_MAPBOX_ACCESS_TOKEN="your_mapbox_access_token"       # Required: Format: pk.eyJ...

# WhatsApp Business API Configuration (Required for WhatsApp Integration)
VITE_WHATSAPP_API_BASE_URL="https://graph.facebook.com/v18.0"  # WhatsApp API base URL
VITE_WHATSAPP_ACCESS_TOKEN="your_whatsapp_access_token"        # Required: WhatsApp Business API access token
VITE_WHATSAPP_PHONE_NUMBER_ID="your_phone_number_id"          # Required: Your WhatsApp Business phone number ID
VITE_WHATSAPP_VERIFY_TOKEN="your_webhook_verify_token"        # Required: Webhook verification token
WHATSAPP_WEBHOOK_SECRET="your_webhook_secret"                 # Required: Webhook signature verification secret

# Stripe Payment Processing (Required for Payments)
VITE_STRIPE_PUBLISHABLE_KEY="your_stripe_publishable_key"     # Required: Format: pk_test_... or pk_live_...
STRIPE_SECRET_KEY="your_stripe_secret_key"                    # Required: Format: sk_test_... or sk_live_...
STRIPE_WEBHOOK_SECRET="your_stripe_webhook_secret"            # Required: Format: whsec_...
STRIPE_CONNECT_CLIENT_ID="your_stripe_connect_client_id"      # Required: Stripe Connect client ID

# Multi-Currency Support (Optional but Recommended for International Users)
VITE_EXCHANGE_RATE_API_KEY="your_exchange_rate_api_key"       # Optional: ExchangeRate-API key for real-time rates
VITE_COMPANY_NAME="TRVL Social"                               # Company name for invoices
VITE_COMPANY_ADDRESS_1="123 Travel Street"                    # Company address line 1
VITE_COMPANY_ADDRESS_2=""                                     # Company address line 2 (optional)
VITE_COMPANY_CITY="San Francisco"                             # Company city
VITE_COMPANY_STATE="CA"                                       # Company state/province
VITE_COMPANY_POSTAL="94102"                                   # Company postal code
VITE_COMPANY_COUNTRY="United States"                          # Company country
VITE_COMPANY_EMAIL="support@trvlsocial.com"                   # Company email for invoices
VITE_COMPANY_PHONE="+1-555-TRVL-HELP"                        # Company phone number
VITE_TAX_NUMBER="TAX123456789"                               # Company tax identification number
VITE_VAT_NUMBER="VAT987654321"                               # Company VAT number (for EU)

# Analytics & Monitoring (Optional but Recommended)
VITE_MIXPANEL_TOKEN="your_mixpanel_token"                     # Optional: Mixpanel analytics token
VITE_SENTRY_DSN="your_sentry_dsn"                            # Optional: Sentry error tracking DSN
VITE_DATADOG_APPLICATION_ID="your_datadog_app_id"            # Optional: Datadog RUM application ID
VITE_DATADOG_CLIENT_TOKEN="your_datadog_client_token"        # Optional: Datadog client token
VITE_DATADOG_SITE="datadoghq.com"                           # Optional: Datadog site (datadoghq.com, datadoghq.eu, etc.)

# Email Configuration (Required for Email Notifications)
SENDGRID_API_KEY="your_sendgrid_api_key"                     # Optional: SendGrid API key for email notifications
FROM_EMAIL="noreply@yourdomain.com"                          # Optional: From email address for notifications